alias D="cd ~/Downloads"
alias g="git"
alias d="docker"

alias java_home="/usr/libexec/java_home"

################################
###  Program ShortCut
################################

# turn on coloring on grep
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

# Quick check running state of a process
alias psef="ps -ef | grep "
